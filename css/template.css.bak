/* CSS Document */

body{
  margin: 0;
  width: 100%;
  font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
  font-size: 15px;
  line-height: 20px;
  color: #333333;
 /*  background-image:url(images/bg-main.jpg) */
 	background-repeat: no-repeat;
	background-position: center;
	background-position: top;
	}

#white{color: white;}
#red{color: red;}
#green{color: #009900;}
#pink{ color: #FF00FF;}
#black{ color: #000;}
#blue{ color: blue;}





#Container-box{
	width: 900px;
	margin: 5px auto 2px  auto;
	height: auto;
	box-shadow:  0px  0px 0px 0px #ddd;
	border:0px solid #ddd; 
	border-radius: 0px;
	padding: 0px;
 
}

 

#index-login{
	width: 100%;
	margin: 0 auto;
	height: auto;
	box-shadow:  0px  0px 0px 0px #ddd;
	border-radius: 0px;
	background-color: #fff;
}

#header{
	width: 100%;
	margin:0 auto 5px auto;
	height: 140px;
	background-image:url(../images/bander.jpg);
	background-position: center;
	background-position: top;
}

#menu{
	width: 100%;
	margin: 0 auto 0 auto;
	height: auto;
	border-top: 0px solid #ddd;
	border-bottom: 0px solid #eee;
}
#ul-box{
	width: 99%;
 	height: auto;
	background: none;
	margin: 0 auto;
	padding: 0px 0px 5px 0px;
}
#container{
	width: 100%;
	height: auto;
	margin:0 auto;
	background: url(../images/bgl-.png) 205px 0px repeat-y; 
	margin-top: 2px;
	
}
#left-content{
	width: 197px;
	height: auto;
	padding: 0px;
	border-radius: 0px;
	float:left;
	margin-left: 2px;
}

#center-content{
	width: 100%;
	margin: 0 auto;
	padding: 10px;
	float: left;
	
	 background-color: #FFFFFF;
	border-radius: 5px 5px 5px 5px;
	-webkit-box-shadow: 0px 0px 5px rgba(0,0,0,.1); 
	-moz-box-shadow: 0px 0px 5px rgba(0,0,0,.1); 
	box-shadow: 0px 0px 5px rgba(0,0,0,.1);	
	
}
#box-content{
	border-bottom: 1px dashed #eee;
	background:#fff;
	margin: 0px;
	
}
#box-table{
border-bottom: 0px solid #3399FF;
margin: 5px 5px 10px 5px;
 

}

#box-form{
	border: 1px dashed #bbb;
	background:#fff;
	padding:10px;
	margin-bottom: 20px;
 
}
#box-list{
	border-bottom: 1px solid #bbb;
	background:#fff;
	padding:10px;
	margin-bottom: 20px;
 	-webkit-box-shadow: 0px 0px 5px rgba(0,0,0,.1); 
	-moz-box-shadow: 0px 0px 5px rgba(0,0,0,.1); 
	box-shadow: 0px 0px 5px rgba(0,0,0,.1);	
}
#right-content{
	width: 222px;
	height:auto;
	padding: 0px;
	background-color: none;
	border-radius: 0px;
	float:right;
}

#footer{
	width: 100%;
	margin: 10px auto 0px auto;
	height: 70px;
	text-align:center;
	border-top: 0px solid #ddd;
}

#box-item{
	border-bottom: 1px solid #ccc;
	margin-left: -1px;
	border-radius: 0px;
	margin-top: -0px;
	background-color: #fff;
}
#box-item-cart{
	width: 520px; 
	font-size: 12px;
	line-height: 14px;
	border:0px;
}
#f-item{
	cursor:default;
	display: inline-block; width: 65px;
	font-weight: bold;
	line-height:15px;
	text-align:right;
	padding-right: 5px;
	font-size: 12px;
}
#f-lb-cart{
	cursor:default;
	display: inline-block; width: 50px;
	font-weight: bold;
	line-height:15px;
	text-align:right;
	padding-right: 5px;
	font-size: 12px;
}

#item{
	width: 360px; 
	font-size: 12px;
	line-height: 14px;
	border:0px;
 
}
#item-am{
	width: 740px; 
	font-size: 15px;
	line-height: 17px;
	border:0px;
 
}
.btn-cart{
	border: 1px solid #ccc;
	border-radius: 3px;
	padding: 4px;
	margin: 2px;
}

.btn-cart:hover{
	border: 1px solid #ccc;
	border-radius: 3px;
	padding: 4px;
	margin: 2px;
 	background: #f4f4f4;
}

